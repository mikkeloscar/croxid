{
    "manifest_version": 2,
    "name": "croxid",
    "version": "0.1",
    "description": "Frontend for proxid",
    "permissions": [
        "proxy",
        "tabs"
    ],
    "background": {
        "scripts": [
            "js/jquery-2.1.4.min.js",
            "js/proxid.js",
            "js/proxy.js",
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19.png",
            "38": "img/icon_38.png"
        },
        "default_title": "croxid ssh proxy",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "img/icon_128.png"
    }
}
